.content{
  margin: 0rpx;
  height: 100%;
  background-color: #EDEDED;
}
.head{
  display: flex;
  margin: 10rpx 0rpx;
  height: 220rpx;
  background-color: rgb(240, 115, 84);
}
.headImgView{
  display: inline-block;
  border-radius: 50%;
  width: 120rpx;
  height: 120rpx;
  overflow:hidden;
  background-color: blue;
  position: relative;
  left:30rpx;
  top: 46rpx;
}
.headImg{
  border-radius: 50%;
  width: 120rpx;
  height: 120rpx;
  overflow:hidden;
}
.headNameView{
  margin-left: 80rpx;
  margin-top: 60rpx;
  display: inline-block;
}
.text{
  color: #d7ec5f;
  font-size: 22rpx;
}
.orderView{
  margin: 20rpx;
  height: 180rpx;
  border-radius: 16rpx;
  background-color:#ffffff;
}
.myOrder{
  color: #515151;
  height: 50rpx;
  padding-top: 16rpx;
  padding-left: 16rpx;
  border-bottom: 2rpx solid #EDEDED;
}
.right-arrow{
  float: right;
  width: 40rpx;
  height: 40rpx;
  margin-right: 36rpx;
}
.orderList{
  display: flex;
  flex-direction: row;
}
.orderItem{
  height: 100rpx;
  width: 100rpx;
  margin: 10rpx 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.order-text{
  color: #515151;
  font-size: 22rpx;
}
.order-img{
  width: 60rpx;
  height: 60rpx;
}
.myListView{
  margin: 20rpx;
  border-radius: 16rpx;
  background-color:#ffffff;
}
.listItem{
  display: flex;
  flex-direction: row;
  margin:10rpx 20rpx;
  height: 100rpx;
  border-bottom: 2rpx solid #EDEDED;
}
.listImg{
  width: 50rpx;
  height: 50rpx;
  margin: 25rpx 15rpx;
}
.listTxt{
  color: #515151;
  font-size: 30rpx;
  height: 50rpx;
  margin: 28rpx 10rpx;
}
.list-right-arrow{
  position: absolute;
  right: 36rpx;
  width: 40rpx;
  height: 40rpx;
  margin: 28rpx 16rpx;
}